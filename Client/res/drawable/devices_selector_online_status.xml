<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/devices_radius_4" />
            <stroke android:width="0.5dip" android:color="@color/devices_green" />
            <solid android:color="@color/devices_green_light" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/devices_radius_4" />
            <solid android:color="@color/devices_black_08" />
        </shape>
    </item>
</selector>
