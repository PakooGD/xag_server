<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="?cube_color_project_secondary" />
            <corners android:radius="@dimen/devices_radius_8" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="?cube_color_project_primary" />
            <corners android:radius="@dimen/devices_radius_8" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/cube_color_container_fill_base" />
            <corners android:radius="@dimen/devices_radius_8" />
        </shape>
    </item>
</selector>
