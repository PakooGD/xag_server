<?xml version="1.0" encoding="utf-8"?>
<layer-list android:layout_width="fill_parent" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/cube_dp_4" />
            <solid android:color="@color/cube_color_container_fill_base" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="@dimen/cube_dp_4" />
                <solid android:color="?cube_color_project_primary" />
            </shape>
        </scale>
    </item>
</layer-list>
