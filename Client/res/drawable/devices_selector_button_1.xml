<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/devices_background" />
            <stroke android:width="0.5dip" android:color="@color/devices_border1" />
            <corners android:radius="@dimen/devices_radius_8" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/devices_background" />
            <stroke android:width="0.5dip" android:color="@color/devices_border1" />
            <corners android:radius="@dimen/devices_radius_8" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/devices_background" />
            <stroke android:width="0.5dip" android:color="@color/devices_border1" />
            <corners android:radius="@dimen/devices_radius_8" />
        </shape>
    </item>
</selector>
