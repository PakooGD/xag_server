<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/face_lib_gray4" />
            <corners android:radius="@dimen/face_lib_px_20" />
            <size android:height="@dimen/face_lib_px_40" android:width="@dimen/face_lib_px_100" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/face_lib_px_20" />
                <solid android:color="@color/face_lib_blue" />
                <size android:height="@dimen/face_lib_px_40" android:width="@dimen/face_lib_px_100" />
            </shape>
        </clip>
    </item>
</layer-list>
