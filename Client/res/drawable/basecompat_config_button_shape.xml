<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="#ff00b405" />
            <solid android:color="#ff00b405" />
            <corners android:radius="23.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="#8800b405" />
            <solid android:color="#8800b405" />
            <corners android:radius="23.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#ffccf0cd" />
            <corners android:radius="23.0dip" />
        </shape>
    </item>
</selector>
