<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/cube_color_on_tertiary" />
            <corners android:radius="@dimen/cube_dp_3" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/cube_color_on_primary" android:endColor="@color/cube_color_on_primary" android:angle="0.0" />
                <corners android:radius="@dimen/cube_dp_3" />
            </shape>
        </clip>
    </item>
</layer-list>
