<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_enabled="true">
        <shape>
            <corners android:radius="@dimen/basecompat_corner_radius_1" />
            <stroke android:width="1.0dip" android:color="@color/basecompat_border" />
            <solid android:color="@color/basecompat_white" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <corners android:radius="@dimen/basecompat_corner_radius_1" />
            <stroke android:width="1.0dip" android:color="@color/basecompat_border" />
            <solid android:color="@color/basecompat_white" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/basecompat_corner_radius_1" />
            <solid android:color="@color/basecompat_disable" />
        </shape>
    </item>
</selector>
