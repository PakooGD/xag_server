<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.25dip" android:color="@color/op_color_content_transp7" />
            <solid android:color="@color/cube_color_on_primary" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="0.25dip" android:color="@color/op_color_content_transp7" />
            <solid android:color="@color/op_color_content_transp6" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.25dip" android:color="@color/op_color_content_transp7" />
            <solid android:color="@color/op_color_container_transp1" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="0.25dip" android:color="@color/op_color_content_transp7" />
            <solid android:color="@color/cube_color_on_primary" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.25dip" android:color="@color/op_color_content_transp7" />
            <solid android:color="@color/cube_color_on_primary" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
</selector>
